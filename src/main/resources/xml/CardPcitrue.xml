<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.jbit.dao.CardPcitrueDao"><!--指定对应得dao层-->
    <cache eviction="LRU" type="cn.jbit.cache.RedisCache"/>

    <select id="findByCid" resultType="CardPcitrue">
        SELECT * FROM cardpcitrue WHERE cId=#{cid}
    </select>
</mapper>
